    "version": "0.2.0",
    "configurations": [
        {
            "name": "holefeeder-ui",
            "request": "launch",
            "cwd": "${workspaceFolder}/holefeeder-flutter",
            "type": "dart",
            "args": [
                "--web-port",
                "3000"
            ]
        },
        {
            "name": "holefeeder-ui (profile mode)",
            "request": "launch",
            "cwd": "${workspaceFolder}/holefeeder-flutter",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--web-port",
                "3000"
            ]
        },
        {
            "name": "holefeeder-ui (release mode)",
            "request": "launch",
            "cwd": "${workspaceFolder}/holefeeder-flutter",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--web-port",
                "3000"
            ]
        }
    ]
}